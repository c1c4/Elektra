<AbsoluteLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/background"
    tools:context="br.com.example.elektrainferface.CozinhaActivity" >

     <ImageView
        android:id="@+id/imgvLampada"
        android:layout_width="247dp"
        android:layout_height="47dp"
        android:layout_x="8dp"
        android:layout_y="8dp"
        android:src="@drawable/lampada" />

    <ToggleButton
        android:id="@+id/btLampadaCozinha"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_x="230dp"
        android:layout_y="12dp"
        android:text="ToggleButton"
        android:textOff="Desligada"
        android:textOn="Ligada" />

    <Button
        android:id="@+id/btVoltarCozinha"
        android:layout_width="305dp"
        android:layout_height="wrap_content"
        android:layout_x="7dp"
        android:layout_y="76dp"
        android:text="Voltar" />

</AbsoluteLayout>